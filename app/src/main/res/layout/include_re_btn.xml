<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:orientation="horizontal"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/text_re_shiled"
        android:text="@string/re_btn01"
        style="@style/re.check"
        android:drawableTop="@mipmap/shiled"
         />
    <TextView
        android:id="@+id/text_re_report"

        style="@style/re.check"
        android:text="@string/re_btn02"
        android:drawableTop="@mipmap/report"

       />
    <TextView
        android:id="@+id/text_re_copyLink"

        style="@style/re.check"
        android:text="@string/re_btn03"
        android:drawableTop="@mipmap/copylink"
        />
    <ImageView
        android:id="@+id/text_re_add"
        style="@style/re.check02"

        />

</RelativeLayout>
